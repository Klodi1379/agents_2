# Environment variables for Docker development
SECRET_KEY=django-dev-secret-key-change-this-in-production
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# Database Configuration - Using SQLite for local development
DB_ENGINE=sqlite
DB_NAME=db.sqlite3
# Uncomment below for PostgreSQL
# DB_USER=postgres
# DB_PASSWORD=postgres
# DB_HOST=localhost
# DB_PORT=5432

# Redis Configuration - Using local Redis or disable for development
REDIS_URL=redis://localhost:6379/0

# AI Service API Keys (Add your real keys here)
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
DEFAULT_LLM_MODEL=gpt-4-turbo-preview
